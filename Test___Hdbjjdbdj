zahl1 = int(input("gib die erste Zahl ein: "))
zahl2 = int(input("gib die zweite Zahl ein: "))
rechenoperator = (input("gib einen Rechenoperator ein, sprich (+, -, *, /): "))


loesung = zahl1, rechenoperator, zahl2
print(loesung)


